    {   "opts": {                                                                   
        "destination": "docs/_jsdoc"                                                              
    },
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [".", 
            "providers/loggly.js",
            "providers/security.js",
            "providers/postgresql.js"],
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}